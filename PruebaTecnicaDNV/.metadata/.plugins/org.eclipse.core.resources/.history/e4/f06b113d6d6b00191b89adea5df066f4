server:
  port: 8081
  
  
spring:
  thymeleaf:
      cache: false
  datasource:
    url: jdbc:mysql://localhost:3306/mydb??zeroDateTimeBehavior=CONVERT_TO_NULL&serverTimezone=UTC
    username : root
    password : root
    
    

  jpa:
    show-sql : true 
    hibernate:
      ddl-auto: update
      naming:
        strategy : org.hibernate.cfg.ImprovedNamingStrategy
    properties:
      hibernate:
       dialect : org.hibernate.dialect.MySQL5Dialect 
       
  
  
  

  
