package com.david.pruebaimatia.utils;

import com.david.pruebaimatia.utils.Status;

/** Creo clase con dos estados para poder cambiarlo si se marca la opciÃ³n de realizado y viceversa
 * 
 * 0 --> tarea incompleta
 * 1 --> tarea realizada
 * 
 **/
public enum Status {

	
	PENDING(0), DONE(1);

	/** The id. */
	private int id;

	
	private Status(int id) {
		this.id = id;
	}

	
	public int getId() {
		return id;
	}

	
	public void setId(int id) {
		this.id = id;
	}

	
	public static Status[] getAll() {
		return Status.values();
	}

}
