package com.david.pruebaimatia.model;

// TODO: Auto-generated Javadoc
/**
 * Creo la clase que va a representar el objeto con sus campos.
 */
public class Task {
	
	
	private int id;
	private String name;
	private boolean is_done = false;
	
	/**
	 * Genero el constructor por defecto, para inicializar los datos a null
	 *
	 */
	public Task() {
		
	}
	
	
	/**
	 * Genero el constructor
	 *
	 * @param id the id
	 * @param name the name
	 * @param is_done the is done
	 */
	public Task(int id, String name, boolean is_done) {
		super();
		this.id = id;
		this.name = name;
		this.is_done = is_done;
	}
	
	/**
	 * Genero getters y setters
	 * 
	 */
	public int getId() {
		return id;
	}


	public void setId(int id) {
		this.id = id;
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}


	public boolean isIs_done() {
		return is_done;
	}


	public void setIs_done(boolean is_done) {
		this.is_done = is_done;
	}
	
	/**
	 * toString
	 */

	@Override
	public String toString() {
		return "Task [id=" + id + ", name=" + name + ", is_done=" + is_done + "]";
	}
	
	
	
	
	
	
	
	
	

}
